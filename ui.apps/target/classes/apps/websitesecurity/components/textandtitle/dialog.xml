<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    title="Text and Title"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <Text_x0020_and_x0020_Title_x0020_details
                jcr:primaryType="cq:Panel"
                title="Text and Title Details">
                <items jcr:primaryType="cq:WidgetCollection">
                    <title
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Enter Title"
                        fieldLabel="Title"
                        maxLength="15"
                        name="./title"
                        xtype="textfield"/>
                    <titlecolor
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Choose the color for Title"
                        fieldLabel="Title color"
                        name="./titlecolor"
                        type="select"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <charcoal_grey
                                jcr:primaryType="nt:unstructured"
                                text="charcoal grey"
                                value="charcoal_grey_font"/>
                            <grey
                                jcr:primaryType="nt:unstructured"
                                text="grey"
                                value="grey_font"/>
                            <lightest_grey
                                jcr:primaryType="nt:unstructured"
                                text="lightest grey"
                                value="lightest_grey_font"/>
                            <light_grey
                                jcr:primaryType="nt:unstructured"
                                text="light grey"
                                value="light_grey_font"/>
                            <midgrey
                                jcr:primaryType="nt:unstructured"
                                text="midgrey"
                                value="midgrey_font"/>
                            <slate_grey
                                jcr:primaryType="nt:unstructured"
                                text="slate_grey"
                                value="slate_grey_font"/>
                            <ash
                                jcr:primaryType="nt:unstructured"
                                text="ash"
                                value="ash_font"/>
                            <Symantec_x0020_Gold
                                jcr:primaryType="nt:unstructured"
                                text="Symantec Gold"
                                value="symantec_gold_font"/>
                            <pink
                                jcr:primaryType="nt:unstructured"
                                text="pink"
                                value="pink_font"/>
                            <blue
                                jcr:primaryType="nt:unstructured"
                                text="sky blue"
                                value="sky_blue_font"/>
                            <black
                                jcr:primaryType="nt:unstructured"
                                text="black"
                                value="black_font"/>
                            <white
                                jcr:primaryType="nt:unstructured"
                                text="white"
                                value="white_font"/>
                            <green
                                jcr:primaryType="nt:unstructured"
                                text="green"
                                value="green_font"/>
                            <purple
                                jcr:primaryType="nt:unstructured"
                                text="purple"
                                value="purple_font"/>
                            <red
                                jcr:primaryType="nt:unstructured"
                                text="red"
                                value="red_font"/>
                        </options>
                    </titlecolor>
                    <subtitle1
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Enter Subtitle"
                        fieldLabel="Sub Title"
                        maxLength="52"
                        name="./subtitle1"
                        xtype="textfield"/>
                    <subtitlecolor
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Choose the color for Subtitle"
                        fieldLabel="Subtitle color"
                        name="./subtitlecolor"
                        type="select"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <charcoal_grey
                                jcr:primaryType="nt:unstructured"
                                text="charcoal grey"
                                value="charcoal_grey_font"/>
                            <grey
                                jcr:primaryType="nt:unstructured"
                                text="grey"
                                value="grey_font"/>
                            <lightest_grey
                                jcr:primaryType="nt:unstructured"
                                text="lightest grey"
                                value="lightest_grey_font"/>
                            <light_grey
                                jcr:primaryType="nt:unstructured"
                                text="light grey"
                                value="light_grey_font"/>
                            <midgrey
                                jcr:primaryType="nt:unstructured"
                                text="midgrey"
                                value="midgrey_font"/>
                            <slate_grey
                                jcr:primaryType="nt:unstructured"
                                text="slate_grey"
                                value="slate_grey_font"/>
                            <ash
                                jcr:primaryType="nt:unstructured"
                                text="ash"
                                value="ash_font"/>
                            <Symantec_x0020_Gold
                                jcr:primaryType="nt:unstructured"
                                text="Symantec Gold"
                                value="symantec_gold_font"/>
                            <pink
                                jcr:primaryType="nt:unstructured"
                                text="pink"
                                value="pink_font"/>
                            <blue
                                jcr:primaryType="nt:unstructured"
                                text="sky blue"
                                value="sky_blue_font"/>
                            <black
                                jcr:primaryType="nt:unstructured"
                                text="black"
                                value="black_font"/>
                            <white
                                jcr:primaryType="nt:unstructured"
                                text="white"
                                value="white_font"/>
                            <green
                                jcr:primaryType="nt:unstructured"
                                text="green"
                                value="green_font"/>
                            <purple
                                jcr:primaryType="nt:unstructured"
                                text="purple"
                                value="purple_font"/>
                            <red
                                jcr:primaryType="nt:unstructured"
                                text="red"
                                value="red_font"/>
                        </options>
                    </subtitlecolor>
                    <Description
                        jcr:primaryType="nt:unstructured"
                        fieldDescription="Enter Description"
                        fieldLabel="Description"
                        name="./description"
                        validateValue="function(field){var temp=this.getValue();if(temp.length>160){this.markInvalid(&quot;Description should not exceed 150 characters&quot;);return false;}else{this.clearInvalid();return true;}}"
                        xtype="richtext">
                        <rtePlugins jcr:primaryType="nt:unstructured">
                            <table
                                jcr:primaryType="nt:unstructured"
                                features="*"/>
                            <format
                                jcr:primaryType="nt:unstructured"
                                features="*"/>
                            <lists
                                jcr:primaryType="nt:unstructured"
                                features="*"/>
                            <justify
                                jcr:primaryType="nt:unstructured"
                                features="*"/>
                            <edit
                                jcr:primaryType="nt:unstructured"
                                features="[paste-wordhtml]"/>
                            <findreplace
                                jcr:primaryType="nt:unstructured"
                                features="*"/>
                            <paraformat
                                jcr:primaryType="nt:unstructured"
                                features="*"/>
                            <subsuperscript
                                jcr:primaryType="nt:unstructured"
                                features="*"/>
                            <misctools
                                jcr:primaryType="nt:unstructured"
                                features="*"/>
                            <links
                                jcr:primaryType="nt:unstructured"
                                features="*"/>
                            <spellcheck
                                jcr:primaryType="nt:unstructured"
                                features="*"
                                invalidStyle="background-color: #ffdddd;"/>
                            <undo
                                jcr:primaryType="nt:unstructured"
                                features="*"/>
                            <image
                                jcr:primaryType="nt:unstructured"
                                features="*"/>
                            <fontstyles
                                jcr:primaryType="nt:unstructured"
                                features="*">
                                <styles jcr:primaryType="cq:WidgetCollection">
                                    <fontstyle
                                        jcr:primaryType="cq:WidgetCollection"
                                        cssName="css"
                                        text="Description"/>
                                </styles>
                            </fontstyles>
                            <fontcolor
                                jcr:primaryType="nt:unstructured"
                                features="*">
                                <styles jcr:primaryType="cq:WidgetCollection">
                                    <fontcolor
                                        jcr:primaryType="cq:WidgetCollection"
                                        cssName="css"
                                        text="Description"/>
                                </styles>
                            </fontcolor>
                            <styles
                                jcr:primaryType="nt:unstructured"
                                features="*">
                                <style
                                    jcr:primaryType="cq:WidgetCollection"
                                    cssName="richtext"
                                    text="Description"/>
                            </styles>
                            <colorpicker
                                jcr:primaryType="nt:unstructured"
                                features="*"/>
                        </rtePlugins>
                    </Description>
                    <Link_x0020_key
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Enter Text for the Link"
                        fieldLabel="Link Title"
                        maxLength="90"
                        name="./linkkey"
                        xtype="textfield"/>
                    <linkpathoverlay
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}true"
                        defaultValue=""
                        fieldDescription="Select Link Path Or Overlay"
                        fieldLabel="&#x9;&#xa;Link Path Or Overlay"
                        name="./linkpathoverlay"
                        type="select"
                        xtype="selection">
                        <listeners
                            jcr:primaryType="nt:unstructured"
                            selectionchanged="function(box,value){var dlg = box.findParentByType('panel');if(value == &quot;link_overlay&quot;){dlg.getComponent('overlayID').show();dlg.getComponent('linkpathID').hide();dlg.getComponent('overlayID1').show();}else{dlg.getComponent('linkpathID').show();dlg.getComponent('overlayID').hide();dlg.getComponent('overlayID1').hide();}}"/>
                        <options jcr:primaryType="cq:WidgetCollection">
                            <path
                                jcr:primaryType="nt:unstructured"
                                text="Link Path"
                                value="linkpath"/>
                            <overlay
                                jcr:primaryType="nt:unstructured"
                                text="Overlay On Link"
                                value="link_overlay"/>
                        </options>
                    </linkpathoverlay>
                    <Link_x0020_value
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Specify the path for the Link"
                        fieldLabel="Link path"
                        itemId="linkpathID"
                        name="./linkvalue"
                        xtype="pathfield"/>
                    <textvidoverlay
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}true"
                        defaultValue=""
                        fieldDescription="Select Text or Video"
                        fieldLabel="Text or Video"
                        itemId="overlayID"
                        name="./textvidoverlay"
                        type="select"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <textoverlay
                                jcr:primaryType="nt:unstructured"
                                text="Text Overlay"
                                value="textoverlay"/>
                            <vidoverlay
                                jcr:primaryType="nt:unstructured"
                                text="Video Overlay"
                                value="vidoverlay"/>
                        </options>
                    </textvidoverlay>
                    <dropdown
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Select Overlay"
                        fieldLabel="Overlay"
                        itemId="overlayID1"
                        name="./dropvalue"
                        options="/bin/websitesecurity/overlayoptions"
                        type="select"
                        value="text"
                        xtype="selection"/>
                </items>
            </Text_x0020_and_x0020_Title_x0020_details>
        </items>
    </items>
</jcr:root>
