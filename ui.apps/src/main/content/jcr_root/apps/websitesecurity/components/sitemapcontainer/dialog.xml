<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    title="Container"
    width="1000"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <colctrl
                jcr:primaryType="cq:Widget"
                title="Column control"
                xtype="panel">
                <items jcr:primaryType="cq:WidgetCollection">
                    <coltype
                        jcr:primaryType="cq:Widget"
                        editable="true"
                        fieldDescription="Column type"
                        fieldLabel="Column type"
                        name="./coltype"
                        type="select"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <col-12
                                jcr:primaryType="nt:unstructured"
                                text="1 Column (100)"
                                value="12-col"/>
                            <col-6-6
                                jcr:primaryType="nt:unstructured"
                                text="2 Columns (50-50)"
                                value="6-6-col"/>
                            <col-3-9
                                jcr:primaryType="nt:unstructured"
                                text="2 Columns(25-75)"
                                value="3-9-col"/>
                            <col-9-3
                                jcr:primaryType="nt:unstructured"
                                text="2 Columns(75-25)"
                                value="9-3-col"/>
                            <col-4-8
                                jcr:primaryType="nt:unstructured"
                                text="2 Columns(30-60)"
                                value="4-8-col"/>
                            <col-8-4
                                jcr:primaryType="nt:unstructured"
                                text="2 Columns(60-30)"
                                value="8-4-col"/>
                            <col-4-4-4
                                jcr:primaryType="nt:unstructured"
                                text="3 Columns(33-33-33)"
                                value="4-4-4-col"/>
                            <col-3-3-6
                                jcr:primaryType="nt:unstructured"
                                text="3 Columns(25-25-50)"
                                value="3-3-6-col"/>
                            <col-3-6-3
                                jcr:primaryType="nt:unstructured"
                                text="3 Columns(25-50-25)"
                                value="3-6-3-col"/>
                            <col-6-3-3
                                jcr:primaryType="nt:unstructured"
                                text="3 Columns(50-25-25)"
                                value="6-3-3-col"/>
                            <col-3-3-3-3
                                jcr:primaryType="nt:unstructured"
                                text="4 Columns(25-25-25-25)"
                                value="3-3-3-3-col"/>
                            <col-2-2-2-2
                                jcr:primaryType="nt:unstructured"
                                text="5 Columns(20-20-20-20-20)"
                                value="2-2-2-2-col"/>
                            <no-value jcr:primaryType="nt:unstructured"/>
                        </options>
                        <listeners jcr:primaryType="nt:unstructured"/>
                    </coltype>
                    <Title
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Enter Title"
                        fieldLabel="Title"
                        maxLength="40"
                        name="./title1"
                        xtype="textfield"/>
                </items>
            </colctrl>
        </items>
    </items>
</jcr:root>
